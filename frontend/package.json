{
  "name": "slapdash-frontend",
  "version": "0.0.1",
  "description": "automatically generate frontend GUI for a slapdash",
  "scripts": {
    "start": "parcel index.html",
    "watch": "parcel watch index.html --out-dir ../slapdash/frontend",
    "build": "parcel build index.html custom.js --out-dir ../slapdash/frontend --no-source-maps --no-content-hash custom.css plugin.css index.js"
  },
  "main": "src/index.tsx",
  "repository": "https://github.com/cathaychris/slapdash",
  "author": "Matt Grau, Chris Axline, Carmelo Mordini",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "7.13.8",
    "@types/lodash": "^4.14.157",
    "@types/node": "^17.0.40",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/socket.io-client": "^1.4.33",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap": "^5.1.3",
    "build-url": "^2.0.0",
    "caniuse-lite": "^1.0.30001364",
    "parcel": "^1.12.4",
    "react": "^16.13.1",
    "react-bootstrap": "^2.2.2",
    "react-bootstrap-range-slider": "^3.0.3",
    "react-collapsible": "^2.8.4",
    "react-dom": "^16.13.1",
    "react-notifications-component": "^4.0.0",
    "react-numeric-input2": "^3.1.0",
    "recharts": "^2.1.10",
    "socket.io-client": "^2.3.0"
  }
}
